<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="@dimen/trans_10px"/>
            <solid android:color="@color/color_font3"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <corners android:radius="@dimen/trans_10px"/>
            <solid android:color="@color/increasing_color_dark"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/trans_10px"/>
            <solid android:color="@color/increasing_color_dark"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <corners android:radius="@dimen/trans_10px"/>
            <solid android:color="@color/increasing_color_dark"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/trans_10px"/>
            <solid android:color="@color/increasing_color"/>
        </shape>
    </item>
</selector>